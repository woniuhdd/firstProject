        </if>
        <if test="@OGNL@isNotEmpty(departname)">
            AND t.DEPARTNAME = #{departname}
        </if>
        <if test="@OGNL@isNotEmpty(contactperson)">
            AND t.CONTACTPERSON = #{contactperson}
        </if>
        <if test="@OGNL@isNotEmpty(contacttel)">
