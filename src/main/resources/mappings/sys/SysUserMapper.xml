    <sql id="common_where_if">
        <if test="@OGNL@isNotEmpty(userid)">
            AND t.USERID = #{userid}
        </if>
        <if test="@OGNL@isNotEmpty(username)">
            AND t.USERNAME = #{username}
        </if>
        <if test="@OGNL@isNotEmpty(userpwd)">
