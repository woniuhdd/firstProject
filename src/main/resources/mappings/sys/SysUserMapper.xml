    <sql id="full-columns">
        t.USERID,
		t.USERNAME,
		t.USERPWD,
		t.USERSTATUS,
		t.LOGINLASTTIME,
		t.ISADMIN,
		t.EXTIME,
		t.ORGID,
		t.CERTID,
