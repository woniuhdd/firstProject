    </sql>

    <insert id="save" parameterType="com.sys.model.SysUser">
        INSERT INTO SYS_USER(
            USERID ,
            USERNAME ,
            USERPWD ,
            USERSTATUS ,
            LOGINLASTTIME ,
            ISADMIN ,
            EXTIME ,
            ORGID ,
            CERTID ,
            MUSTCA ,
            ISCOMPANY ,
            DEPARTNAME ,
            CONTACTPERSON ,
            CONTACTTEL ,
            ID ,
            CERTSIGN ,
            USERTYPE ,
            ADMINUSERID ,
            UPDATEUSERNAME ,
            UPDATEUSERPOSITION ,
            UPDATEUSERDEPARTMENT ,
            UPDATEUSERPHONE ,
            UPDATEUSERTEL
        )
        VALUES (
                   #{userid} ,
                   #{username} ,
                   #{userpwd} ,
                   #{userstatus} ,
                   #{loginlasttime} ,
                   #{isadmin} ,
                   #{extime} ,
                   #{orgid} ,
                   #{certid} ,
                   #{mustca} ,
                   #{iscompany} ,
                   #{departname} ,
                   #{contactperson} ,
                   #{contacttel} ,
                   #{id} ,
                   #{certsign} ,
                   #{usertype} ,
                   #{adminuserid} ,
                   #{updateusername} ,
                   #{updateuserposition} ,
                   #{updateuserdepartment} ,
                   #{updateuserphone} ,
                   #{updateusertel}
               )
    </insert>

    <insert id="saveBatch" parameterType="java.util.List">
        INSERT INTO SYS_USER (
        USERID ,
        USERNAME ,
        USERPWD ,
        USERSTATUS ,
        LOGINLASTTIME ,
        ISADMIN ,
        EXTIME ,
        ORGID ,
        CERTID ,
        MUSTCA ,
        ISCOMPANY ,
        DEPARTNAME ,
        CONTACTPERSON ,
        CONTACTTEL ,
        ID ,
        CERTSIGN ,
        USERTYPE ,
        ADMINUSERID ,
        UPDATEUSERNAME ,
        UPDATEUSERPOSITION ,
        UPDATEUSERDEPARTMENT ,
        UPDATEUSERPHONE ,
        UPDATEUSERTEL
        ) VALUES
        <foreach collection="list" item="item" index="index"
                 separator=",">
            (
            #{item.userid},
            #{item.username},
            #{item.userpwd},
            #{item.userstatus},
            #{item.loginlasttime},
            #{item.isadmin},
            #{item.extime},
            #{item.orgid},
            #{item.certid},
            #{item.mustca},
            #{item.iscompany},
            #{item.departname},
            #{item.contactperson},
            #{item.contacttel},
            #{item.id},
            #{item.certsign},
            #{item.usertype},
            #{item.adminuserid},
            #{item.updateusername},
            #{item.updateuserposition},
            #{item.updateuserdepartment},
            #{item.updateuserphone},
            #{item.updateusertel}
            )
        </foreach>
    </insert>

    <select id="getById" parameterType="java.lang.String" resultMap="BaseResultMap">
        SELECT <include refid="full-columns" />
        FROM SYS_USER t
        WHERE t.USERID = #{value}
    </select>

    <select id="getUserByUsername" parameterType="java.lang.String"
            resultMap="BaseResultMap">
        SELECT
        <include refid="full-columns" />
        FROM SYS_USER t
        WHERE t.USERID = #{value}
    </select>

    <select id="getListByParams" parameterType="map" resultMap="BaseResultMap">
        SELECT
        <include refid="full-columns" />
        FROM SYS_USER t
        <where>
            <include refid="common_where_if"/>
        </where>
    </select>

    <select id="countByParams" parameterType="map" resultType="int">
        SELECT count(*)
        FROM SYS_USER t
        <where>
            <include refid="common_where_if"/>
        </where>
    </select>

